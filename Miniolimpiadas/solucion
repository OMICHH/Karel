class program {
    define rodear(){
        turnleft();
        move();
        iterate(3){
        	turnleft();
        }
        move();
        iterate(3){
        	turnleft();
        }
        move();
        turnleft();
    }

    program () {
        iterate(9){
        	if(frontIsBlocked){
            	rodear();
            }else{
            	move();
            }
        }
        turnoff();
    }
}
